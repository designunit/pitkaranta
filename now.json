{
    "version": 2,
    "name": "pitkaranta",
    "builds": [
        {
            "src": "/readymag/**",
            "use": "@now/static"
        }
    ],
    "routes": [
        {
            "src": "/(\\d)/?",
            "dest": "/readymag/index.html"
        },
        {
            "src": "^/[.a-zA-Z0-9-]+(/(.*))?",
            "dest": "/readymag/$2"
        }
    ]
}